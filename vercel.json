{
    "buildCommand": "npm run build",
    "outputDirectory": "public",
    "routes": [
        {
            "handle": "filesystem"
        },
        {
            "src": "/img/(.*)",
            "headers": {
                "cache-control": "public, max-age=2592000, immutable"
            }
        },
        {
            "src": "/zStyles/webfonts/(.*)",
            "headers": {
                "cache-control": "public, max-age=2592000, immutable"
            }
        },
        {
            "src": "/.well-known/discord",
            "status": 302,
            "dest": "/zDesk/discord.html"
        }
    ]
}
